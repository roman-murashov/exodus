        MENUITEM "Close ROM",                   ID_FILE_CLOSEROM
        MENUITEM SEPARATOR
        MENUITEM "Load State\tShift+F8",        ID_FILE_LOADSTATE
        MENUITEM "Save State\tShift+F5",        ID_FILE_SAVESTATE
