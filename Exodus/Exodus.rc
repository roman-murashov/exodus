            MENUITEM "9\tCtrl+9",                   ID_SELECTSTATESLOT_9
            MENUITEM "0\tCtrl+0",                   ID_SELECTSTATESLOT_0
        END
        MENUITEM SEPARATOR
        MENUITEM "Load Workspace\tCtrl+W",      ID_FILE_LOADWORKSPACE
        MENUITEM "Save Workspace",              ID_FILE_SAVEWORKSPACE
